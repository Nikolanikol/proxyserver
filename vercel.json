{
    "version" : 2,
    "buils": [
        {
            "src" : "index.js",
            "use" : "@versel/node"
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "index.js",
            "methods" : [ "GET", "POST", "DELETE"],
            "headers" : {
                "Access-Control-Allow-Origin" : "*"
            }
        }
    ]
}